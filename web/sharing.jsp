<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
    </head>
    <body>
        <div class="right-side col-3">
            <div class="sharing">
                <div class="sharing-heading">
                    <h4 class="text-sharing-heading">Share this page</h4>
                </div>
                <div class="sharing-content">
                    <ul class="content-wrapper">
                        <li class="sharing__item">
                            <div class="sharing__social-icon icon-facebook"> </div>
                            <a id="share-facebook" href="#">Share on Facebook</a>
                        </li>
                        <li class="sharing__item">
                            <div class="sharing__social-icon icon-twitter"> </div>
                            <a id="share-twitter" href="#">Share on Twitter</a>
                        </li>
                        <li class="sharing__item">
                            <div class="sharing__social-icon icon-google-plus"> </div>
                            <a id="share-google-plus" href="#">Share on Google</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </body>
</html>
